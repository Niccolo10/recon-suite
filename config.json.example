{
  "tools": {
    "httpx_path": "httpx",
    "threads": 50,
    "timeout": 10,
    "retries": 2,
    "ports": [80, 443, 8080, 8443],
    "rate_limit": 150
  },
  
  "passive": {
    "microsoft_ti": {
      "enabled": false,
      "endpoint_domain": "https://prod.eur.ti.trafficmanager.net/api/dns/passive/subdomains/export?query={domain}",
      "endpoint_count": "https://prod.eur.ti.trafficmanager.net/api/dns/passive/subdomains/count?query={domain}",
      "request_interval": 10,
      "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64)",
      "timeout": 3600,
      "processes_input": [
        {
          "authorization": "Bearer YOUR_TOKEN_HERE",
          "proxy": null
        }
      ]
    },
    
    "securitytrails": {
      "enabled": false,
      "sec_id": "YOUR_SEC_ID_HERE",
      "endpoint_apex": "https://securitytrails.com/_next/data/{sec_id}/list/apex_domain/{domain}.json?page={page}&domain={domain}",
      "request_interval": 15,
      "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:145.0) Gecko/20100101 Firefox/145.0",
      "timeout": 3600,
      "processes_input": [
        {
          "cookie": "YOUR_COOKIES_HERE",
          "proxy": null
        }
      ]
    },
    
    "crtsh": {
      "enabled": true,
      "timeout": 30
    },
    
    "sublist3r": {
      "enabled": true,
      "threads": 40,
      "engines": null
    },

    "google_dork": {
      "enabled": false,
      "api_keys": ["YOUR_GOOGLE_API_KEY"],
      "cx": "YOUR_CUSTOM_SEARCH_ENGINE_ID",
      "dorks": [
        "ext:log",
        "ext:env",
        "ext:sql",
        "ext:bak",
        "ext:conf",
        "ext:config",
        "inurl:admin",
        "inurl:api",
        "intitle:index.of"
      ],
      "results_per_query": 10,
      "request_interval": 2,
      "timeout": 30
    }
  },
  
  "discovery": {
    "js_analyzer": {
      "timeout": 30,
      "max_file_size_mb": 10
    },
    "wayback": {
      "rate_limit": 2,
      "timeout": 30
    }
  },
  
  "analysis": {
    "reflection": {
      "timeout": 10
    },
    "errors": {
      "timeout": 10
    }
  },
  
  "vulns": {
    "takeover": {
      "nuclei_path": "nuclei",
      "subzy_path": "subzy",
      "threads": 25,
      "timeout": 10,
      "use_nuclei": true,
      "use_subzy": true
    },
    "misconfig": {
      "checks": ["cors", "git", "debug_endpoints"]
    }
  }
}
